[user]
  name = Kishan Karunaratne
  email = kishkaru@gmail.com
[credential]
  helper = cache --timeout=3600
[core]
  autocrlf = input
  safecrlf = true
[push]
  default = nothing
[color]
  ui = true
[alias]
  br = branch
  ci = commit
  co = checkout
  st = status
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
